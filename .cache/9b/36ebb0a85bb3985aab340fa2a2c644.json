-motion: reduce) {\r\n  * {\r\n    animation-duration: 0.01ms !important;\r\n    animation-iteration-count: 1 !important;\r\n    transition-duration: 0.01ms !important;\r\n    scroll-behavior: auto !important;\r\n  }\r\n}\r\n\r\n\r\nbody {\r\n    background-color: #88BCE3;\r\n    font-family: 'Open Sans Condensed', sans-serif;\r\n}\r\n.list {\r\n    margin: 0 auto;\r\n    max-width: 400px;\r\n   margin-top: 50px;\r\n}\r\n.block {\r\n    -webkit-box-shadow: 4px 4px 11px 22px rgba(14, 15, 16, 0.2);\r\n    -moz-box-shadow: 4px 4px 11px 22px rgba(14, 15, 16, 0.2);\r\n    box-shadow: 4px 4px 11px 22px rgba(14, 15, 16, 0.2);\r\n}\r\n.p__text {\r\n    padding: 20px 30px 20px 30px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    background-color: orange;\r\n}\r\n.p__enter { \r\n    font-size: 30px;\r\n    color: #E2EAF9;\r\n    font-weight: 400;\r\n}\r\n.enter__word {\r\n    width: 100%;\r\n    height: 30px;\r\n    padding: 20px\r\n}\r\n.new__block {\r\n  background-color: #FFFFFF;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  transition: 0.5s;\r\n  margin-top: 8px;\r\n}\r\n.new__text {\r\n    padding: 20px;\r\n    padding-right: 180px;\r\n}\r\n.write__image {\r\n    height: 100%;\r\n    width: 45px;\r\n}\r\n.delete {\r\n    width: 52px;\r\n    height: 100%;\r\n    padding-right: 5px;\r\n}\r\n\r\n.new__block:hover {\r\n  transition: 0.5s;\r\n  background-color:#EDF1F5;\r\n}"},"lineCount":null}},"error":null,"hash":"765a42756575706b431b32091106ebfd","cacheData":{"env":{}}}